# <wasabi --hooks argument>;<output directory name>
# comments and empty lines are ignored

;none

#start,nop,unreachable,memory_size,memory_grow;basic
start;start
nop;nop
unreachable;unreachable
memory_size;memory_size
memory_grow;memory_grow

#drop,select;drop+select
drop;drop
select;select

const;const
global;global
local;local

#load,store;load+store
load;load
store;store

#unary,binary;unary+binary
unary;unary
binary;binary

#call,return;call+return
call;call
return;return

#br,br_if,br_table;branches
br;br
br_if;br_if
br_table;br_table

#if,begin,end;blocks
if;if
begin;begin
end;end
